programa 
{
   
  funcao inicio()
   {
    inteiro matricula[8], nr_matricula=0
    cadeia nome_aluno[8],nomes,sobrenomes
    real nota(1),nota(2),nota(3),nota(4),nota(5),nota(6),nota(7),nota(8)

    para(inteiro posicao = 0; posicao <8; posicao++)

    { 
      escreva("Insira a matricula do aluno",posicao,"\n")
      leia(nr_matricula)
      matricula[posicao] = nr_matricula
      escreva("\n Digite o nome do aluno:")
      leia(nomes)
      nome_aluno[posicao]=nomes
      escreva("\n Digite o sobrenome do aluno:")
      leia(sobrenomess)
      sobrenome_aluno[posicao]=sobrenomes
      limpa()
    }

    escreva("Vetor na ordem original:\n")
    para(inteiro posicao =0; posicao <8; posicao++)
    
    {
     escreva(matricula[posicao]," ")
    }
  
  }
}
